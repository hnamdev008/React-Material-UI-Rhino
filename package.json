{
  "name": "Skiff",
  "version": "0.1.0",
  "description": "Proprietary Front-End Application for use with Rhino Security Labs Sandbar project",
  "author": "Rhino Security Labs",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "http://rhinosecuritylabs.com",
  "config": {
    "env": "local"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "install:latest": "npm install --save-dev typescript@* istanbul@* jasmine@*",
    "init": "rm -rf .git && npm run install:latest && npm init && git init",
    "dist": "npm run build",
    "prebuild": "npm run clean && mkdir -p dist && cp prod_index/index.html dist/",
    "build": "./node_modules/.bin/webpack -p --config webpack.config.prod.js",
    "postbuild": "cp -r assets dist/ && mkdir dist/assets/js",
    "setup:mock": "cd mock && npm run teardown:mock && docker-compose up -d",
    "teardown:mock": "cd mock && docker-compose down",
    "test": "rm -rf build && npm run build:tsc && istanbul cover jasmine",
    "posttest": "istanbul check-coverage --statements 80 --functions 80 --branches 80 --lines 80",
    "publish:major": "npm version major && npm run prepack && npm publish build",
    "publish:minor": "npm version minor && npm run prepack && npm publish build",
    "publish:patch": "npm version patch && npm run prepack && npm publish build",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "start": "node server.js"
  },
  "dependencies": {
    "@types/history": "^2.0.48",
    "@types/jasmine": "^2.5.47",
    "@types/jwt-decode": "^1.4.28",
    "@types/material-ui": "^0.17.4",
    "@types/node": "^7.0.14",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.40",
    "@types/react-router": "^2.0.49",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-form": "^6.6.2",
    "@types/webpack": "^2.2.15",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "browserify-fs": "^1.0.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "geopattern": "^1.2.3",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.1.0",
    "material-ui": "^0.18.0",
    "moment": "^2.15.1",
    "moment-timezone": "^0.5.7",
    "node-sass": "^4.0.0",
    "normalize.css": "^5.0.0",
    "popsicle": "^8.2.0",
    "postcss-loader": "^1.1.0",
    "prop-types": "^15.5.6",
    "react": "^15.5.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-data-grid": "^2.0.33",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.7.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "babel-plugin-istanbul": "^3.0.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve-url-loader": "^2.0.2",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.1.6",
    "webpack-dev-server": "^2.4.5"
  },
  "main": "index.js"
}
